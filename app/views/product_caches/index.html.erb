<p id="notice"><%= notice %></p>

<h1>Product Caches</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Name</th>
      <th>Values</th>
      <th>Date created</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_caches.each do |product_cach| %>
      <tr>
        <td><%= product_cach.key %></td>
        <td><%= product_cach.name %></td>
        <td><%= product_cach.values %></td>
        <td><%= product_cach.date_created %></td>
        <td><%= link_to 'Show', product_cach %></td>
        <td><%= link_to 'Edit', edit_product_cach_path(product_cach) %></td>
        <td><%= link_to 'Destroy', product_cach, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Cach', new_product_cach_path %>
